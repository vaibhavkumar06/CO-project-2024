def decimal_to_binary_32(decimal):
    decimal=int(decimal)
    return "0b"+bin(decimal)[2:].zfill(16) 

def binary_to_decimal(binary):
    return int(binary, 2)

mem_address = {}
for i in range(128):
    bin_str = format(i, '07b')  
    mem_address[bin_str] ="0000000000000000"
input = []
for i in sys.stdin:
    i = i.rstrip() 
    words = i.split()
    input.append(words)
mem_address_pointer=0   
for m in input:
    mem_address[decimal_to_binary_7 (mem_address_pointer)]=m[0]
    mem_address_pointer+=1
pc=0
while pc<len(input):
    op=input[pc][0]
    op=op[25:32]
    i=input[pc][0]

    if op=="0110011" and i[17:20]=="000" and i[0:7]=="0000000": 
        p=pc
        reg=i[20:25]
        reg1=i[12:17]
        reg2=i[7:12]
        
        Registers[reg]=decimal_to_binary_32( binary_to_decimal(Registers[reg1][8:])  +   binary_to_decimal(Registers[reg2][8:])   )
        pc=pc+1
